<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/changes/1.0.0"
    xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/plugins/maven-changes-plugin/xsd/changes-1.0.0.xsd">
  <body>

    <release version="1.2.0" date="2024-03-13">
      <action type="update" dev="sseifert">
        Update to JSONPath 2.9.0.
      </action>
    </release>

    <release version="1.1.0" date="2021-10-26">
      <action type="update" dev="sseifert">
        Update to JSONPath 2.6.0.
      </action>
      <action type="update" dev="sseifert">
        Explicitly define versions of optional package dependencies.
      </action>
    </release>

    <release version="1.0.2" date="2021-01-18">
      <action type="update" dev="sseifert">
        org.osgi.framework was embedded by accident.
      </action>
    </release>

    <release version="1.0.0" date="2021-01-17">
      <action type="update" dev="sseifert">
        Update to JSONPath 2.5.0.
      </action>
      <action type="update" dev="sseifert">
        Switch to Java 8.
      </action>
      <action type="update" dev="sseifert">
        Switch to individual OSGI artifacts.
      </action>
    </release>

    <release version="0.5.0" date="2015-05-29">
      <action type="update" dev="sseifert">
        Initial release.
      </action>
    </release>

  </body>
